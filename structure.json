{
    "format": "json",
    "quanitity": 10,
    "show": false,
    "sql_settings": 
    {
        "execute": false,
        "host": "host",
        "user": "user",
        "password": "password",
        "database": "database",
        "tablename": "test"
    },
    "csv_settings": 
    {
        "delimiter": ",",
        "newline": ";\n"
    }
    ,"xml_settings": 
    {
        "newline": "\n"
    },
    "quote": "'",
    "http_settings":
    {
        "execute": false,
        "port": 7567
    },
    "fields":
    [
        {
            "label": "id",
            "value": "integer",
            "autoIncrementFrom": 1
        },
        {
            "label": "name",
            "value": "first_name"
        },
        {
            "label": "lastName",
            "value": "last_name",
            "lenght": 2
        },
        {
            "label": "active",
            "value": "integer",
            "range": [0,2]
        },
        {
            "label": "employeeId",
            "value": "integer",
            "range": [0,1000]
        },
        {
            "label": "date",
            "value": "date",
            "range": ["01-01-2000","24-12-2040"],
            "format": "%d-%m-%Y"
        },
        {
            "label": "timestamp",
            "value": "date",
            "range": ["1/1/1990 00:00:00","31/12/2080 00:00:00"],
            "format": "%d/%m/%Y %H:%M:%S"
        },
        {
            "label": "site",
            "value": "places"
        },
        {
            "label": "emoji",
            "value": "emoji",
            "length": 10
        }
    ]
}